package calculator;

public class Calculations {
	
	public int add(int a, int b) {
		return a+b;
	}
	
	public int subtract(int a, int b) {
		return a-b;
	}
	
	public int multiply(int a, int b) {
		return a*b;
	}
	
	public int divide(int a, int b) {
		if(a==0) {
			System.out.println("Undefined");
		}
		else 
			return a/b;
		
		
	}
}
